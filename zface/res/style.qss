QRadioButton {
/*     border: 2px solid #8f8f91;*/
     background-color: qlineargradient(spread:pad, x1:1, y1:0, x2:1, y2:1, stop:0 rgba(159, 175, 191, 255), stop:1 rgba(232, 232, 234, 255));
     border-radius: 5px;
     min-width: 80px;
     padding: 3px;
     margin: 2 px;
 }

QRadioButton::indicator {
     width: 16px;
     height: 16px;
     /*border-color: blue;*/
     /*color: blue;*/
}

QRadioButton::indicator::checked {
     image: url(:/all/res/radio_on.png);
}

QRadioButton::indicator::unchecked {
     image: url(:/all/res/radio_off.png);
}


